{
  "compilerOptions": {
    "sourceMap": true, // 소스맵(*.map) 파일 생성 여부
    "jsx": "preserve", // Resolve: Cannot use JSX unless '--jsx' flag is provided
    "esModuleInterop": true // import * as => import
    ,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
